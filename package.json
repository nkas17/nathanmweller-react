{
  "name": "react-starter",
  "version": "1.1.0",
  "description": "react starter with webpack and babel",
  "main": "index.js",
  "repository": "https://github.com/nkas17/nathanmwller-react.git",
  "author": "nathan m weller <nkas17@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --config webpack.config.prod.js",
    "build-develop": "webpack",
    "clean": "npx rimraf dist",
    "format": "prettier --check \"**/*.*\"",
    "lint": "eslint . --ext .jsx --ext .js src webpack.config.*",
    "lint:report": "eslint . --ext .jsx --ext .js src --output-file ./report/eslint-report.html --format html",
    "test": "jest",
    "pre-push": "yarn lint && yarn format && yarn test && yarn clean && yarn build"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@testing-library/jest-dom": "5.15.1",
    "@testing-library/react": "12.1.2",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "copy-webpack-plugin": "10.0.0",
    "css-loader": "6.5.1",
    "eslint": "8.3.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "jest": "27.3.1",
    "mini-css-extract-plugin": "2.4.5",
    "prettier": "2.5.0",
    "react-test-renderer": "17.0.2",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "webpack": "5.64.4",
    "webpack-dev-server": "4.5.0"
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-router-dom": "5.3.0",
    "react-router-hash-link": "2.4.3",
    "webpack-cli": "4.9.1"
  },
  "jest": {
    "modulePaths": [
      "/styles"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  }
}
