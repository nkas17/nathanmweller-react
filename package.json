{
  "name": "nathanmweller-react",
  "version": "1.1.0",
  "description": "nathanmweller website",
  "main": "index.js",
  "repository": "https://github.com/nkas17/nathanmwller-react.git",
  "author": "nathan m weller <nkas17@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --config webpack.config.prod.js",
    "build-develop": "webpack",
    "clean": "npx rimraf dist",
    "format": "prettier --check \"**/*.*\"",
    "lint": "eslint . --ext .jsx --ext .js src webpack.config.*",
    "lint:report": "eslint . --ext .jsx --ext .js src --output-file ./report/eslint-report.html --format html",
    "test": "jest",
    "pre-push": "npm run lint && npm run format && npm run test && npm run clean && npm run build",
    "update": "npx npm-check-updates"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "@types/react": "17.0.40",
    "@types/react-dom": "17.0.13",
    "@types/react-router-dom": "5.3.3",
    "@types/react-router-hash-link": "2.4.5",
    "@types/webpack": "5.28.0",
    "@types/webpack-dev-server": "4.7.1",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.3",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "jest": "27.5.1",
    "mini-css-extract-plugin": "2.6.0",
    "prettier": "2.5.1",
    "react-refresh": "0.11.0",
    "react-test-renderer": "17.0.2",
    "style-loader": "3.3.1",
    "type-fest": "2.12.0",
    "typescript": "4.6.2",
    "url-loader": "4.1.1",
    "webpack": "5.70.0",
    "webpack-dev-server": "4.7.4"
  },
  "dependencies": {
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "6.2.2",
    "react-router-hash-link": "2.4.3",
    "webpack-cli": "4.9.2"
  },
  "jest": {
    "modulePaths": [
      "/styles"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom"
  }
}
